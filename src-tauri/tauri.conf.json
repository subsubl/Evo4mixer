{
  "productName": "Evo4mixer",
  "version": "0.1.0",
  "identifier": "com.ator.evo4mixer",
  "build": {
    "beforeDevCommand": "cd frontend && trunk serve --address 0.0.0.0 --port 1420",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "cd frontend && trunk build --dist ../src-tauri/target/frontend",
    "frontendDist": "target/frontend"
  },
  "app": {
    "windows": [
      {
        "title": "Evo4mixer",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
